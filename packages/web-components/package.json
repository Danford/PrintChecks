{
  "name": "@printchecks/web-components",
  "version": "1.0.1",
  "description": "Framework-agnostic Web Components for PrintChecks",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist",
    "custom-elements.json"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "type-check": "tsc --noEmit"
  },
  "keywords": [
    "web-components",
    "custom-elements",
    "printchecks",
    "checks",
    "receipts",
    "framework-agnostic"
  ],
  "author": "Joshua Danford",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Danford/PrintChecks.git",
    "directory": "packages/web-components"
  },
  "homepage": "https://github.com/Danford/PrintChecks#readme",
  "publishConfig": {
    "access": "public"
  },
  "sideEffects": true,
  "unpkg": "./dist/index.global.js",
  "jsdelivr": "./dist/index.global.js",
  "dependencies": {
    "@printchecks/core": "workspace:*"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3"
  },
  "customElements": "custom-elements.json"
}